{
  "name": "template-cloudflare-workers-react",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build --minify",
    "preview": "bun run build && vite preview",
    "cf:dev": "wrangler dev",
    "cf:typegen": "wrangler types",
    "cf:check": "tsc && vite build && wrangler deploy --dry-run",
    "cf:deploy": "bun run build && wrangler types && wrangler deploy",
    "check": "biome check --write src --verbose",
    "check:unsafe": "biome check --write src --unsafe --verbose",
    "db:touch": "wrangler d1 execute betterlist-db --local --command='SELECT 1'",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply betterlist-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply betterlist-db --remote",
    "db:studio": "drizzle-kit studio",
    "db:studio:prod": "ENVIRONMENT=production drizzle-kit studio"
  },
  "dependencies": {
    "@biomejs/biome": "^1.9.4",
    "@hono/trpc-server": "^0.3.4",
    "@hono/zod-validator": "^0.7.0",
    "@libsql/client": "^0.15.5",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.6",
    "@tanstack/react-form": "^1.12.0",
    "@tanstack/react-query": "^5.75.7",
    "@tanstack/react-router": "^1.120.3",
    "@trpc/client": "^11.1.2",
    "@trpc/server": "^11.1.2",
    "@trpc/tanstack-react-query": "^11.1.2",
    "better-auth": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "hono": "4.7.7",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.503.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "resend": "^4.5.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.6",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.0.8",
    "@cloudflare/workers-types": "^4.20250529.0",
    "@tanstack/react-query-devtools": "^5.75.7",
    "@tanstack/react-router-devtools": "^1.120.3",
    "@tanstack/router-plugin": "^1.120.3",
    "@types/node": "22.14.1",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.4.1",
    "drizzle-kit": "^0.31.1",
    "globals": "15.15.0",
    "tw-animate-css": "^1.2.9",
    "typescript": "5.8.3",
    "vite": "6.3.2",
    "wrangler": "^4.14.4"
  },
  "packageManager": "bun@1.2.12"
}
